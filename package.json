    "execa": "^5.1.1",
    "jest": "^27.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "vercel": "^23.0.0",
    "yorkie": "^2.0.0"
