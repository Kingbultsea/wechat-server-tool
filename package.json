    "@types/koa": "^2.13.3",
    "chokidar": "^3.5.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "lru-cache": "^6.0.0",
    "madge": "^5.0.1",
    "minimist": "^1.2.5",
    "superagent": "^7.0.2",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
